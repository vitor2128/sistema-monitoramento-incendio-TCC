{
  "name": "estacao-meteorologica-api",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Vitor Hugo <hugo.veigav@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir ./dist/src --extensions \".ts,.js\" --copy-files",
    "start": "node dist/src/shared/infra/http/server.js",
    "dev": "tsnd -r tsconfig-paths/register --inspect --ignore-watch node_modules --respawn --transpile-only src/shared/infra/http/server.ts",
    "knex:migrate:make": "npx knex --knexfile src/knexfile.ts -x ts migrate:make",
    "knex:migrate:latest": "npx knex --knexfile src/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "npx knex --knexfile src/knexfile.ts migrate:rollback",
    "knex:seed:make": "npx knex seed:make",
    "knex:seed:run": "npx knex seed:run"
  },
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/node": "^7.20.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.0",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "knex": "^2.3.0",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tsyringe": "^4.7.0",
    "twilio": "^3.83.3",
    "typescript": "^4.8.4"
  }
}
